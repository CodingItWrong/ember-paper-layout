<LayoutContainer @title="My Ember App" as |layout|>
  <layout.sidenav>
    This is the sidebar!
  </layout.sidenav>
  <layout.body as |body|>
    <body.toolbar @showSideNavToggle={{this.session.isAuthenticated}}>
      This is the toolbar!
    </body.toolbar>
    <body.content>
      {{outlet}}
    </body.content>
  </layout.body>
</LayoutContainer>
